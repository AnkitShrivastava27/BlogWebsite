{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/api/index.js",
            "methods": ["Get","Post","Put","Patch","Delete","Options"]
        },
        {
            "src": "/api/(.*)",
            "dest": "server/api/index.js"
          }
    ]
}